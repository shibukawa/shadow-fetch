{
    "name": "shadow-fetch",
    "version": "0.2.1",
    "description": "fake fetch/httpServer for SSR",
    "main": "index.js",
    "browser": "browser.js",
    "scripts": {
        "test": "nyc ava -v",
        "benchmark": "ava -v --serial",
        "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
        "security-test": "snyk test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shibukawa/shadow-fetch"
    },
    "keywords": [
        "isomorphic",
        "fetch",
        "next.js",
        "express.js"
    ],
    "author": "Yoshiki Shibukawa",
    "license": "MIT",
    "devDependencies": {
        "ava": "^0.25.0",
        "browserify": "^16.2.0",
        "codecov": "^3.0.0",
        "eslint": "^4.19.1",
        "express": "^4.16.3",
        "flow-bin": "^0.70.0",
        "node-fetch": "^2.1.2",
        "nyc": "^11.6.0",
        "override-require": "^1.1.1",
        "snyk": "^1.78.1",
        "streamtest": "^1.2.3",
        "unfetch": "^3.0.0"
    },
    "ava": {
        "files": [
            "test/test-*.js"
        ],
        "source": [
            "lib/**/*.{js,jsx}",
            "index.js"
        ]
